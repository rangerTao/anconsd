<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ranger.bmaterials"
    android:installLocation="internalOnly"
    android:versionCode="100"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="8" />

    <!-- 必须申明的权限 -->
    <!-- 百度移动统计需要的权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="com.android.launcher.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- <uses-permission android:name="android.permission.INSTALL_PACKAGES" /> -->
    <uses-permission android:name="android.permission.ACCESS_SUPERUSER" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE"/>

    <!-- 可选的权限 -->
    <!-- 启用错误分析功能，须添加该权限 -->
    <uses-permission android:name="android.permission.READ_LOGS" />

    <!-- 百度push的权限 -->
    <!-- 监听系统启动的权限,和上面重复 -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <!-- <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" /> -->
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <!-- Add permission for baidu account -->
    <permission
        android:name="com.baidu.permission.SHARE"
        android:description="@string/sapi_share_description"
        android:label="@string/sapi_share_label"
        android:permissionGroup="android.permission-group.PERSONAL_INFO"
        android:protectionLevel="dangerous" />

    <uses-permission android:name="com.baidu.permission.SHARE" />

    <application
        android:name=".app.GameTingApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- 百度移动统计 -->
        <!-- 渠道商编号，其中name请不要改，将value修改为渠道商编号。渠道名称请到mtj.baidu.com的渠道管理中修改 -->
        <meta-data
            android:name="BaiduMobAd_CHANNEL"
            android:value="202" />
        <meta-data
            android:name="BaiduMobAd_STAT_ID"
            android:value="6499e1e234" />

        <!-- 以下是可选的 -->
        <!-- 是否开启错误日志统计，默认为f alse -->
        <meta-data
            android:name="BaiduMobAd_EXCEPTION_LOG"
            android:value="true" />
        <!-- 日志发送策略，可选值：APP_START、ONCE_A_DAY、SET_TIME_INTERVAL，默认为APP_START -->
        <meta-data
            android:name="BaiduMobAd_SEND_STRATEGY"
            android:value="APP_START" />
        <!-- 上面的日志发送策略 为SET_TIME_INTERVAL时，需设定时间间隔。取值为1-24（小时）的整数，默认为1 -->
        <meta-data
            android:name="BaiduMobAd_TIME_INTERVAL"
            android:value="1" />
        <!-- 日志仅在wifi网络下发送，默认为false -->
        <meta-data
            android:name="BaiduMobAd_ONLY_WIFI"
            android:value="false" />
        <!-- 是否获取基站位置信息，默认为true(获取基站位置) -->
        <meta-data
            android:name="BaiduMobAd_CELL_LOCATION"
            android:value="false" />
        <!-- 是否获取GPS位置信息，默认为获取GPS位置但需开启FINE_LOCATION权限 -->
        <meta-data
            android:name="BaiduMobAd_GPS_LOCATION"
            android:value="false" />
        <!-- 是否获取WIFI位置信息，默认为true(获取WIFI位置) -->
        <meta-data
            android:name="BaiduMobAd_WIFI_LOCATION"
            android:value="true" />

        <activity
            android:name=".ui.MainHallActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.topicdetail.TopicDetailActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.MineEditActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.MineGamesActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.MineMsgActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.MineMsgDetailActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ui.BMMineCollectionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.MineSettingsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.BMLoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.BMRegisterActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.RegisterProtocolActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.FeedbackActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.ForgetPwdActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.BindPhoneActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.BindPhoneVerifyActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.ChangePwdActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ui.gametopic.BMProductDetailActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.AboutUsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.ManagerActivity"
            android:exported="true"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.GameGuideDetailActivity2"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.IgnoredUpdatableAppsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.ActivityDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.AppraisalDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.OpenServerDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.SnapNumberDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.MoreGameActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.MoreClassGameActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.GameDetailsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.ui.PublishCommentActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ui.BMSearchResultActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.ExchangeHistoryAcitivty"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ranger.bmaterials.ui.DownloadDialogActivity"
            android:screenOrientation="portrait"
            android:theme="@style/dialog_activity_theme" />
        <activity
            android:name="com.ranger.bmaterials.ui.WebviewActivity"
            android:screenOrientation="portrait" />
        <activity

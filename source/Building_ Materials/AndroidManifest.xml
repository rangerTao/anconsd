            android:screenOrientation="portrait" />
        <service
            android:name="com.ranger.bmaterials.tools.install.SilentInstallService"
            android:process=":installservice" />
        <service
            android:name="com.ranger.bmaterials.uservice.DKUserInfoRemoteService"
            android:process=":dkuservice" >
            <intent-filter>
                <action android:name="com.ranger.bmaterials.uservice.IDKUserService" />
            </intent-filter>
        </service>

        <receiver android:name="com.ranger.bmaterials.broadcast.AppMonitorReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED" />
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <action android:name="android.intent.action.PACKAGE_CHANGED" />

                <data android:scheme="package" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MEDIA_MOUNTED" />
                <action android:name="android.intent.action.MEDIA_UNMOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_COMPLETE" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_PAUSE" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_START" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_PAUSE_BY_USER" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_CANCLE" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.ranger.bmaterials.broadcast.PackageReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_COMPLETE" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_PAUSE" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_START" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_PAUSE_BY_USER" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_CANCLE" />
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_RUNNING" />
            </intent-filter>
        </receiver>

        <provider
            android:name="com.ranger.bmaterials.download.DownloadProvider"
            android:authorities="com.ranger.bmaterials.download"
            android:exported="true" />

        <service android:name="com.ranger.bmaterials.download.DownloadService" />

        <receiver
            android:name="com.ranger.bmaterials.download.DownloadReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MEDIA_MOUNTED" />

                <data android:scheme="file" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.ranger.bmaterials.download.OpenDownloadReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.ranger.bmaterials.intent.action.DOWNLOAD_NOTIFICATION_CLICKED" />

                <data
                    android:mimeType="vnd.android.cursor.item/download"
                    android:scheme="content" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.ranger.bmaterials.broadcast.PushServiceReceiver" >
            <intent-filter>
                <action android:name="com.baidu.android.pushservice.action.MESSAGE" />
                <action android:name="com.baidu.android.pushservice.action.RECEIVE" />
                <action android:name="com.baidu.android.pushservice.action.notification.CLICK" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.baidu.android.pushservice.PushServiceReceiver"
            android:process=":push_service" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="com.baidu.android.pushservice.action.notification.SHOW" />
                <action android:name="com.baidu.android.pushservice.action.media.CLICK" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.baidu.android.pushservice.RegistrationReceiver"
            android:process=":push_service" >
            <intent-filter>
                <action android:name="com.baidu.android.pushservice.action.METHOD" />
                <action android:name="com.baidu.android.pushservice.action.BIND_SYNC" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.baidu.android.pushservice.PushService"
            android:process=":push_service" />

        <activity
            android:name="com.ranger.bmaterials.download.SizeLimitActivity"
            android:configChanges="orientation|keyboardHidden|screenSize|screenLayout"
            android:launchMode="singleTask"
            android:theme="@android:style/Theme.Translucent" />

        <uses-library android:name="android.test.runner" />

        <receiver android:name="com.ranger.bmaterials.updateservice.BootBroadcastReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <service android:name="com.ranger.bmaterials.updateservice.CheckGameUpdateService" />
        <service
            android:name="com.ranger.bmaterials.speeddownload.SpeedDonwloadService"
            android:exported="true"
            android:process="com.ranger.bmaterials.speedservice" />

        <receiver android:name=".broadcast.SpeedDownloadReceiver" >
            <intent-filter>
                <action android:name="com.ranger.bmaterials.speeddownload.REQUESTRECEIVER" />
            </intent-filter>
        </receiver>
        <receiver android:name=".broadcast.UnInstallReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".ui.CompetitionActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.ranger.bmaterials.sapi.SapiLoginActivity"
            android:launchMode="singleTask" />

        <receiver
            android:name="com.baidu.sapi2.share.ShareReceiver"
            android:permission="com.baidu.permission.SHARE" >
            <intent-filter>
                <action android:name="baidu.intent.action.SHARE" />
                <action android:name="baidu.intent.action.NEWSHARE" />
                <action android:name="baidu.intent.action.SHARE_V6" />
            </intent-filter>
        </receiver>

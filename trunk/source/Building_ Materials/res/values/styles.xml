<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/bm_authorized_provider_selector</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:maxHeight">10.0dip</item>
    </style>

    <style name="cb_autologin" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/bm_autologin_selector</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:maxHeight">10.0dip</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="android:Theme.Light">

        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    
    <style name="rule_text_view">
        <item name="android:layout_marginLeft">12dp</item>
        <item name="android:layout_marginRight">12dp</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Theme.Start" parent="android:Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="dialog_style_base" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowTitleStyle">@null</item>
        <item name="android:windowSoftInputMode">stateUnchanged|adjustResize</item>
    </style>

    <style name="translucent">
        <item name="android:windowBackground">@color/transparent2</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="content_list">
        <item name="android:background">@drawable/background_full_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:divider">@null</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="content_list_seach">
        <item name="android:background">@drawable/background_full_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:headerDividersEnabled">false</item>
        <item name="android:footerDividersEnabled">false</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:divider">@null</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="content_list_item_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="content_list_item_exchange_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666666</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="content_list_item_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#969696</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="update_list_item_text_style_incremental_size">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FB720D</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="content_list_item_imageview_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
    </style>

    <style name="button_style">
        <item name="android:textColor">#5B5B5B</item>
        <item name="android:textSize">15sp</item>
.</style>

    <style name="content_list_item_linear_layout_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="content_list_item_progressbar_style" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">8dp</item>
        <item name="android:max">100</item>
    </style>

    <style name="pull_header_TextAppearance" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#6666</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="search_hight_light_TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#000</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="dialog_activity_theme" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="scroll_tab_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">#ffffff</item>
    </style>

    <!-- <item name="android:background">@drawable/scroll_tab_bg</item> -->

    <style name="list_sep_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left|center_vertical</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="list_sep_text_style_nogravity">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#666</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="list_sep_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">@dimen/manager_layout_padding_horizontal</item>
        <item name="android:paddingLeft">@dimen/manager_layout_padding_horizontal</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:background">#D2D2D2</item>
        <item name="android:textSize">12sp</item>
        <item name="android:minHeight">23dp</item>
    </style>

    <style name="list_sep_layout_style_for_manager">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">@dimen/manager_layout_padding_horizontal</item>
        <item name="android:paddingLeft">@dimen/manager_layout_padding_horizontal</item>
        <item name="android:background">#D2D2D2</item>
        <item name="android:textSize">12sp</item>
        <item name="android:minHeight">23dp</item>
    </style>

    <style name="manager_btn_layout_layout_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">#EAEAEA</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">@dimen/manager_layout_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/manager_layout_padding_horizontal</item>
    </style>

    <style name="suggest_style" parent="@android:style/Widget.ListView.DropDown">
        <item name="android:background">@drawable/transparent_drawable</item>
        <item name="android:cacheColorHint">@android:color/transparent</item>
        <item name="android:divider">@null</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <!--
      <item name="android:divider">@drawable/bg_divider_home</item>
    <item name="android:colorBackgroundCacheHint">@null</item>
    -->
    <!-- <item name="android:windowBackground">@drawable/edit_tag_bg</item> -->

    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Activity</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="exchange_card_text_style">
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="NotificationText">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>

    <style name="NotificationTitle">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="check_update_itemName_TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="check_update_itemValue_TextAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>

</resources>